    "@opentelemetry/tracing": "^0.13.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "aws-serverless-express": "^3.3.6",
    "body-parser": "^1.19.0",
    "domready": "^1.0.8",
    "express": "^4.17.1",
    "hashids": "1.1.1",
    "morgan": "^1.10.0",
    "request": "^2.88.2",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-runtime": "^7.13.6",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@pollyjs/adapter-node-http": "^2.7.0",
    "@pollyjs/core": "^2.6.3",
