    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "browserify": "^12.0.1",
    "chai": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "d3": "^4.13.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "json-diff": "^0.5.4",
    "nodemon": "^1.19.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "sinon": "^7.5.0",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "uglify-js": "^2.7.3",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.5.0"
  },
  "scripts": {
    "build": "webpack --config configs/webpack.config.js --mode production",
    "build:dev": "webpack --config configs/webpack.config.js --mode development",
    "start": "npm run build:dev && node app.js",
    "test": "jest --config configs/jest.config.js",
    "watch": "nodemon -w src -w app.js -w package.json --exec make",
    "lambda:zip": "zip -r function.zip . -x=.git/*",
    "lambda:update": "aws lambda update-function-code --function-name L0 --zip-file fileb://function.zip",
    "deploy:lambda": "npm run build && npm run lambda:zip && npm run lambda:update",
    "deploy:gcloud": "gcloud functions deploy L0 --trigger-http --runtime=nodejs10 --entry-point=compiler"
  }
}
